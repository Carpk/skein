@font-face {
    font-family: 'Fixedsys500c';
    src: url(<%= asset_path('assets/Fixedsys500c.eot') %>);
    src: url(<%= asset_path('assets/Fixedsys500c.eot?#iefix') %>) format('embedded-opentype'),
         url(<%= asset_path('assets/Fixedsys500c.woff') %>) format('woff'),
         url(<%= asset_path('assets/Fixedsys500c.ttf') %>) format('truetype'),
         url(<%= asset_path('assets/Fixedsys500c.svg#akagithin') %>) format('svg');
    font-weight: 300;
    font-style: normal;
}

body {
  padding-top: 60px;
}

.box {
  height: 7.3em;
  background-color: black;
}

.navbar-default .navbar-brand {
  color: #000084;
}

.navbar-default .navbar-brand:hover{
  color: #BBBBBB;
  background-color: black;
}

a:focus {
  background: transparent;
}

.navbar-default .navbar-nav > li > a:focus {
  background-color: transparent;
  color: black;
  outline: none;
}

.text-field {
  overflow: hidden;
}

.rooms {
  height: 134px;
  width: 217px;
  white-space: pre;
  margin-left: auto 0;
  margin-right: auto 0;
}

